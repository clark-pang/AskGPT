
{
  "name": "Action script injection demo",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_title": "Click to show an alert",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    { "js": ["content.js"],
    "matches": ["<all_urls>"]
     }
  ],
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  }
}